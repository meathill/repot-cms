        "images",
        "files"
      ]
    },
    "logo_url": {
      "type": "string"
    },
    "is_featured": {
      "type": "boolean"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "mappedBy": "protocols"
    },
